    "prettier-standard": "^9.1.1",
    "prettylint": "^1.0.0",
    "puppeteer": "^1.11.0",
    "request": "^2.88.0",
    "sass-loader": "^6.0.7",
    "selenium-server": "^3.14.0",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "standard-reporter": "^1.0.5",
    "standard-tap": "^1.0.4",
