    "cookie-parser": "^1.4.3",
    "cors": "^2.5.2",
    "docusign-esign": "^5.8.1",
    "docusign-rooms": "^1.0.0",
    "dotenv": "^5.0.1",
    "event-stream": "^4.0.1",
    "formik": "^1.5.8",
    "helmet": "^1.3.0",
    "i18next": "^11.10.2",
    "i18next-browser-languagedetector": "^2.2.3",
    "i18next-express-middleware": "^1.3.2",
    "i18next-node-fs-backend": "^2.1.0",
