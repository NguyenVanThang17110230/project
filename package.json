    "stylelint-scss": "^3.3.0",
    "twilio": "^3.39.5"
  },
  "repository": {
    "url": "https://gitlab.com/dayone-teams/int-boilerplates/int-loopnext.git"
  },
  "standard": {
    "ignore": [
      ".dist",
      "client/static"
    ],
    "parser": "babel-eslint",
    "globals": [
      "Morris",
      "describe",
      "$",
      "test",
      "jest",
      "expect",
      "it",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "beforeAll",
      "afterAll",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "localStorage",
      "React$Element",
      "alert",
      "Promise",
      "confirm"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "> 0.5%",
    "Last 4 versions",
    "Firefox ESR",
    "iOS 8"
  ]
}
