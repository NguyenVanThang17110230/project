{
  "name": "loopnext",
  "description": "Loopback.js + Next.js scaffolding",
  "version": "5.0.1",
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "scripts": {
    "dev": "nodemon --exec babel-node server/server.js",
    "doc": "cd docs/website && docusaurus-start --port 3001",
    "start": "node server/server.js",
    "lint": "standard | snazzy && stylelint client/scss && prettylint \"{client,common,server,test,docs}/**/*.{json,md}\"",
    "lint:fix": "prettier-standard \"{client,common,server,test,docs}/**/*.js\" && prettier --write \"{client,common,server,test,docs}/**/*.{json,md}\" && standard --fix | snazzy && stylelint client/scss --fix",
    "lint:report": "npm run lint:js:report && npm run lint:scss:report",
    "lint:js:report": "rm -rf .lint-report && mkdir -p .lint-report && standard | standard-reporter --checkstyle > .lint-report/lint-js.xml",
    "lint:scss:report": "stylelint client/scss --custom-formatter=node_modules/stylelint-checkstyle-formatter > .lint-report/lint-scss.xml",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --coverage --coverageDirectory=.test-report-unit",
    "test:e2e": "rm -rf .test-report-e2e && next build client && nightwatch --config test/e2e/nightwatch.conf.js",
    "audit": "npm audit",
    "build": "rm -rf .dist .dist-bundle && npm run build:commonAndServer && npm run build:client && mkdir -p .dist-bundle && tar -cvzf .dist-bundle/bundle.tar.gz .dist",
    "build:commonAndServer": "babel common -d .dist/common --ignore \"common/**/*.test.js\" && cp -R server .dist/server && cp package.json package-lock.json ecosystem.json .dist && babel server -d .dist/server --ignore \"common/**/*.test.js\"",
    "build:client": "rm -rf client/.next && mkdir -p .dist/client && next build client && cp -R client/.next .dist/client/.next && cp -R client/static .dist/client/static && cp client/*.js .dist/client",
    "heroku-postbuild": ""
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@fullcalendar/core": "^4.3.1",
    "@fullcalendar/daygrid": "^4.3.0",
    "@fullcalendar/interaction": "^4.3.0",
    "@fullcalendar/moment": "^4.3.0",
    "@fullcalendar/moment-timezone": "^4.3.0",
    "@fullcalendar/react": "^4.3.0",
    "@fullcalendar/timegrid": "^4.3.0",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.2-canary.2",
    "aws-sdk": "^2.513.0",
    "axios": "^0.18.0",
    "base64url": "^3.0.1",
